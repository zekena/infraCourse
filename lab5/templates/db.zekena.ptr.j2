$TTL	604800
@	IN	SOA	zekena.ttu. root.zekena.ttu. (
			      4		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
{% for i in dns_records %}
{% if i.type == "NS" %}
 IN {{ i.type }} {{ i.address }}
{% elif i.type == "A" %}
{{ i.address.split('.')[3] }} IN PTR {{ i.name }}
{% endif %}
{% endfor %}
